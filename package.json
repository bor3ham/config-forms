{
  "name": "config-forms-demo",
  "version": "0.0.1",
  "description": "Demo site for config-forms.",
  "main": "index.js",
  "repository": "https://github.com/uptick/config-forms",
  "author": "Uptick",
  "license": "MIT",
  "private": false,
  "scripts": {
    "publish:demo": "git branch -D gh-pages && git push origin --delete gh-pages; git checkout -b gh-pages && yarn && yarn build && git add . && git add -f dist && git add -f node_modules/uptick-demo-site/dist && git commit -m \"Demo site build\" && git push origin gh-pages && git checkout master && git push origin `git subtree split --prefix demo gh-pages`:gh-pages --force",
    "build:js": "./node_modules/.bin/webpack",
    "build:css": "./node_modules/stylus/bin/stylus demo.styl -o dist/demo.css --include-css",
    "watch:css": "./node_modules/stylus/bin/stylus demo.styl -o dist/demo.css --include-css --watch",
    "watch:js": "./node_modules/.bin/webpack --watch",
    "watch": "./node_modules/.bin/concurrently \"npm run watch:js\" \"npm run watch:css\"",
    "start": "./node_modules/.bin/concurrently \"npm run watch\" \"jekyll serve\"",
    "build": "npm run build:js && npm run build:css"
  },
  "dependencies": {
    "config-forms-react": "^0.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "uptick-demo-site": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "stylus": "^0.54.8",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  }
}
